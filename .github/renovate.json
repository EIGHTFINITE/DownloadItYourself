{
  "extends": [
    "config:base",
    ":rebaseStalePrs",
    ":disableDependencyDashboard"
  ],
  "labels": ["dependencies", "do-not-merge"],
  "packageRules": [
    {
      "matchPackageNames": ["electron", "node"],
      "labels": ["dependencies", "automerge"],
      "automerge": true,
      "automergeType": "pr",
      "automergeStrategy": "fast-forward",
      "ignoreTests": true
    }
  ],
  "ignoreDeps": ["npm-6"]
}
